package com.cmc.repaso.entidades;

public class Producto {
	private String nombre;
	private int precio;

	public Producto(String nombre, int precio) {
		this.nombre = nombre;
		this.precio = precio;
	}

	public void setPrecio(int precio) {
		if (precio < 0) {
			this.precio = precio * -1;
		} else {
			this.precio = precio;
		}
	}
}