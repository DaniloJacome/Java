package com.krakedev;

public class TestCuadrado {

	public static void main(String[] args) {
		Cuadrado r1=new Cuadrado();
		Cuadrado p1=new Cuadrado();
		
		double areaR1;
		double perimetroR1;
		
		r1.lado=4;
		p1.lado=3;
		
		areaR1=r1.calcularArea();
		perimetroR1=p1.calcularPerimetro();
		System.out.println("El area del Cuadrado es: "+areaR1);
		System.out.println("El perimetro del Cuadrado es: "+perimetroR1);

	}

}
